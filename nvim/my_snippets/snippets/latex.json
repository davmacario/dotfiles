{
  "Template": {
    "prefix": "template-basic",
    "body": [
      "\\documentclass[12pt]{article}",
      "\\usepackage[utf8]{inputenc}",
      "",
      "\\usepackage{hyperref}   % Change color and style of \\ref",
      "\\hypersetup{",
      "\tcolorlinks=true,",
      "\tlinkcolor=blue,",
      "\tfilecolor=magenta,",
      "\turlcolor=cyan,",
      "\tpdftitle={Overleaf Example},",
      "\tpdfpagemode=FullScreen,",
      "}",
      "",
      "\\usepackage{graphicx} % Allows you to insert figures",
      "\\graphicspath{ {./images/} } % images are found in this position",
      "\\counterwithin{figure}{section} % Settings for figure numbering",
      "\\counterwithin{figure}{subsection}",
      "",
      "\\usepackage{amsmath} % Allows you to do equations",
      "\\usepackage{amsfonts} % Contains math. symbols fonts (e.g., 'real numbers' set)",
      "\\usepackage{fancyhdr} % Formats the header",
      "\\usepackage{geometry} % Formats the paper size, orientation, and margins",
      "\\linespread{1.25} % about 1.5 spacing in Word",
      "\\setlength{\\parindent}{0pt} % no paragraph indents",
      "\\setlength{\\parskip}{1em} % paragraphs separated by one line",
      "",
      "\\usepackage{enumitem} % Used to reduce whitespace between list elements",
      "\\setlist[itemize]{noitemsep, topsep=0pt} % Set the whitespace above list to the minimum",
      "",
      "\\usepackage{algorithm}",
      "\\usepackage{algpseudocodex}",
      "",
      "\\usepackage[style=authoryear-ibid,backend=biber,maxbibnames=99,maxcitenames=2,uniquelist=false,isbn=false,url=true,eprint=false,doi=true,giveninits=true,uniquename=init]{biblatex} % Allows you to do citations - does Harvard style and compatible with Zotero",
      "\\urlstyle{same} % makes a nicer URL and DOI font",
      "\\AtEveryBibitem{",
      "\t\\clearfield{urlyear}",
      "\t\\clearfield{urlmonth}",
      "} % removes access date",
      "\\AtEveryBibitem{\\clearfield{month}} % removes months in bibliography",
      "\\AtEveryCitekey{\\clearfield{month}} % removes months in citations",
      "\\renewbibmacro{in:}{} % Removes the 'In' before journal names",
      "",
      "\\renewbibmacro*{editorstrg}{%from biblatex.def",
      "  \\printtext[editortype]{%",
      "\t\\iffieldundef{editortype}",
      "\t  {\\ifboolexpr{",
      "\t\ttest {\\ifnumgreater{\\value{editor}}{1}}",
      "\t\tor",
      "\t\ttest {\\ifandothers{editor}}",
      "\t\t}",
      "\t\t{\\bibcpstring{editors}}",
      "\t\t{\\bibcpstring{editor}}}",
      "\t{\\ifbibxstring{\\thefield{editortype}}",
      "\t\t{\\ifboolexpr{",
      "\t\t\ttest {\\ifnumgreater{\\value{editor}}{1}}",
      "\t\t\tor",
      "\t\t\ttest {\\ifandothers{editor}}",
      "\t\t\t}",
      "\t\t\t{\\bibcpstring{\\thefield{editortype}s}}%changed",
      "\t\t  {\\bibcpstring{\\thefield{editortype}}}}%changed",
      "\t\t{\\thefield{editortype}}}}}",
      "",
      "\\renewbibmacro*{byeditor+others}{%from biblatex.def",
      "  \\ifnameundef{editor}",
      "\t{}",
      "\t{\\printnames[byeditor]{editor}%",
      "\t \\addspace%added",
      "\t \\mkbibparens{\\usebibmacro{editorstrg}}%added",
      "\t \\clearname{editor}%",
      "\t \\newunit}%",
      "  \\usebibmacro{byeditorx}%",
      "  \\usebibmacro{bytranslator+others}}",
      "  % The commands above from lines 20-49 change the way editors are displayed in books",
      "\\AtEveryBibitem{%",
      "  \\clearlist{language}%",
      "} % removes language from bibliography",
      "% Removes ibids (ibidems)",
      "\\DeclareNameAlias{sortname}{family-given} % Ensures the names of the authors after the first author are in the correct order in the bibliography",
      "\\renewcommand*{\\revsdnamepunct}{} % Corrects punctuation for authors with just a first initial",
      "%\\addbibresource{Example.bib} % Tells LaTeX where the citations are coming from. This is imported from Zotero",
      "\\usepackage[format=plain,",
      "\t\t\tfont=it]{caption} % Italicizes figure captions",
      "\\usepackage[english]{babel}",
      "\\usepackage{csquotes}",
      "\\renewcommand*{\\nameyeardelim}{\\addcomma\\space} % Adds comma in in-text citations",
      "\\renewcommand{\\headrulewidth}{0pt}",
      "\\geometry{letterpaper, portrait, margin=1in}",
      "\\setlength{\\headheight}{14.49998pt}",
      "",
      "\\newcommand\\titleofdoc{${1:Default Title}} %%%%% Title",
      "\\newcommand\\GroupName{${2:Default Author}}",
      "\\newcommand\\CurrDate{${3:Date}}",
      "",
      "\\begin{document}",
      "\\begin{titlepage}",
      "   \\begin{center}",
      "\t\t\\vspace*{4cm} % Adjust spacings to ensure the title page is generally filled with text",
      "",
      "\t\t\\Huge{\\titleofdoc}",
      "",
      "\t\t\\vspace{0.5cm}",
      "\t\t\\LARGE{${4:Default Subtitle}}",
      "",
      "\t\t\\vspace{3 cm}",
      "\t\t\\Large{\\GroupName\\\\\\ }",
      "\t\t\\large{${5:Affiliation}}",
      "",
      "",
      "\t\t\\vspace{2 cm} % Optional additional info here",
      "",
      "",
      "\t\t\\vspace{3 cm}",
      "\t\t\\Large{\\CurrDate}",
      "",
      "\t\t\\vspace{0.25 cm}",
      "\t\t\\Large{A.Y.\\ ${6:Academic Year}}",
      "",
      "",
      "\t\t\\vfill",
      "\t\\end{center}",
      "\\end{titlepage}",
      "",
      "\\setcounter{page}{2}",
      "\\pagestyle{fancy}",
      "\\fancyhf{}",
      "\\rhead{\\thepage}",
      "\\lhead{\\GroupName; \\titleofdoc}",
      "",
      "% Text here",
      "",
      "\\end{document}"
    ],
    "description": "Generate a simple LaTeX template"
  },
  "Cheatsheet template": {
    "prefix": "template-cheatsheet",
    "body": [
      "\\documentclass[9pt,landscape]{extarticle}",
      "\\usepackage{amssymb,amsmath,amsthm,amsfonts}",
      "\\usepackage{multicol,multirow}",
      "\\usepackage{calc}",
      "\\usepackage{ifthen}",
      "\\usepackage[landscape]{geometry}",
      "\\usepackage[colorlinks=true,citecolor=blue,linkcolor=blue]{hyperref}",
      "\\usepackage{extsizes}",
      "",
      "\\ifthenelse{\\lengthtest{ \\paperwidth= 11in}}",
      "\t{\\geometry{top=.5in,left=.5in,right=.5in,bottom=.5in}}",
      "\t{\\ifthenelse{ \\lengthtest{ \\paperwidth= 297mm}}",
      "\t\t{\\geometry{top=1cm,left=1cm,right=1cm,bottom=1cm} }",
      "\t\t{\\geometry{top=1cm,left=1cm,right=1cm,bottom=1cm} }",
      "}",
      "\\pagestyle{empty}",
      "\\makeatletter",
      "\\renewcommand{\\section}{\\@startsection{section}{1}{0mm}%",
      "\t\t\t\t\t\t\t{-1ex plus -.5ex minus -.2ex}%",
      "\t\t\t\t\t\t\t{0.5ex plus .2ex}%x",
      "\t\t\t\t\t\t\t{\\normalfont\\large\\bfseries}}",
      "\\renewcommand{\\subsection}{\\@startsection{subsection}{2}{0mm}%",
      "\t\t\t\t\t\t\t{-1explus -.5ex minus -.2ex}%",
      "\t\t\t\t\t\t\t{0.5ex plus .2ex}%",
      "\t\t\t\t\t\t\t{\\normalfont\\normalsize\\bfseries}}",
      "\\renewcommand{\\subsubsection}{\\@startsection{subsubsection}{3}{0mm}%",
      "\t\t\t\t\t\t\t{-1ex plus -.5ex minus -.2ex}%",
      "\t\t\t\t\t\t\t{1ex plus .2ex}%",
      "\t\t\t\t\t\t\t{\\normalfont\\small\\bfseries}}",
      "\\makeatother",
      "\\setcounter{secnumdepth}{0}",
      "\\setlength{\\parindent}{0pt}",
      "\\setlength{\\parskip}{0pt plus 0.3ex}",
      "% -----------------------------------------------------------------------",
      "",
      "\\title{${1:title}}",
      "\\author{${2:author}}",
      "\\begin{document}",
      "",
      "\\raggedright\\footnotesize",
      "",
      "\\makeatletter",
      "\\begin{center}",
      "\t\\Large{\\@title} \\",
      "\\end{center}",
      "\\makeatother",
      "\\begin{multicols}{3}",
      "\\setlength{\\premulticols}{1pt}",
      "\\setlength{\\postmulticols}{1pt}",
      "\\setlength{\\multicolsep}{1pt}",
      "\\setlength{\\columnsep}{2pt}",
      "",
      "\\end{multicols}",
      "",
      "\\end{document}"
    ]
  },
  "Figure": {
    "prefix": "figure",
    "body": [
      "\\begin{figure}[ht]",
      "\t\\centering",
      "\t\\includegraphics[width=${1:width}]{${2:path}}",
      "\t\\caption{${3:caption}}\\label{${4:label}}",
      "\\end{figure}"
    ],
    "description": "Insert a figure"
  },
  "Algorithm": {
    "prefix": "algorithm",
    "body": [
      "\\begin{algorithm}",
      "\t\\caption{${1:caption}}\\label{${2:label}}",
      "\t\\begin{algorithmic}[${3:line-start-value}]",
      "\t\t\\State ${4:algorithm}",
      "\t\\end{algorithmic}",
      "\\end{algorithm}"
    ],
    "description": "Insert an algorithm"
  },
  "Minipage": {
    "prefix": "minipage",
    "body": [
      "\\begin{figure} [ht]",
      "\t\\begin{minipage}{0.45\\linewidth}",
      "\t\\centering",
      "\t\t\\includegraphics[width=\\textwidth]{${1:img-1-path}}",
      "\t\t\\caption{${2:caption-1}}\\label{${3:label-1}}",
      "\t\\end{minipage}",
      "\t\\hspace{0.5cm}",
      "\t\\begin{minipage}{0.45\\linewidth}",
      "\t\\centering",
      "\t\t\\includegraphics[width=\\textwidth]{${4:img-2-path}}",
      "\t\t\\caption{${5:caption-2}}\\label{${6:label-2}}",
      "\t\\end{minipage}",
      "\\end{figure}"
    ],
    "description": "Minipage environment for two images side-to-side"
  },
  "Table": {
    "prefix": "table",
    "body": [
      "\\begin{table}[ht]",
      "\t\\centering",
      "\t\\begin{tabular}{|c|c|}",
      "\t\t\\hline",
      "\t\t\\textbf{${1:col-1}} & \\textbf{2:col-2} \\\\\\ ",
      "\t\t\\hline",
      "\t\t\\hline",
      "\t\t${3:val-1} & ${4:val-2} \\\\\\",
      "\t\t\\hline",
      "\t\\end{tabular}",
      "\t\\caption{${5:caption}}",
      "\t\\label{${6:label}}",
      "\\end{table}"
    ],
    "description": "Insert table"
  }
}
