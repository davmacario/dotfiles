{
  "name": "my-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": ["latex", "tex"],
        "path": "./snippets/latex.json"
      },
      {
        "language": "python",
        "path": "./snippets/python.json"
      },
      {
        "language": "arduino",
        "path": "./snippets/arduino.json"
      },
      {
        "language": ["markdown", ".md", ".mdx"],
        "path": "./snippets/markdown.json"
      }
    ]
  }
}
