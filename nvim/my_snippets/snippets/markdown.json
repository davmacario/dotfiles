{
  "YAML header": {
    "prefix": "header",
    "body": [
      "---",
      "title: ${1:title}",
      "author: Davide Macario",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "---"
    ]
  },
  "Table of Contents": {
    "prefix": ["table-of-content", "table_of_content", "toc"],
    "body": ["[[_TOC_]]"]
  },
  "Obsidian header": {
    "prefix": "obsidian-header",
    "body": [
      "---",
      "id: ${1:title}",
      "author: Davide Macario",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "aliases: []",
      "tags: []",
      "---"
    ]
  }
}
